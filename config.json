{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include":[
            "docs/js",
            "./README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "destination": "./out",
        "recurse": true,
        "verbose": true,
        "tutorials": ""
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}